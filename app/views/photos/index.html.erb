<% if @logged_in_as %>
  <%= link_to "Post new photo", :new_photo, class: "button new" %>
<% end %>

<% if @photos.present? %>
  <section id="gallery">
    <% @photos.each do |photo| %>
      <%= render "shared/photo_thumbnail", photo: photo %>
    <% end %>
  </section>
<% else %>
  <h1>No image found</h1>
<% end%>
